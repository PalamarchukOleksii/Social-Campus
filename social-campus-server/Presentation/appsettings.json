{
  "Logging": {
    "LogLevel": {
      "Default": "Warning",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ResetPasswordToken": {
    "ExpirationInSeconds": 600
  },
  "EmailVerificationToken": {
    "ExpirationInSeconds": 600
  },
  "Hasher": {
    "SaltSize": 32,
    "HashSize": 64,
    "Iterations": 600000
  },
  "Jwt": {
    "Issuer": "socialcampus-api",
    "Audience": "socialcampus-users",
    "AccessTokenExpirationInSeconds": 60,
    "RefreshTokenExpirationInSeconds": 259200
  }
}
